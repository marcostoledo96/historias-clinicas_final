{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/[[...slug]].js" },
    { "handle": "filesystem" },
    { "src": "/components/(.*)", "dest": "/frontend/components/$1" },
    { "src": "/js/(.*)", "dest": "/frontend/js/$1" },
    { "src": "/css/(.*)", "dest": "/frontend/css/$1" },
    { "src": "/images/(.*)", "dest": "/frontend/images/$1" },
    { "src": "/favicon.ico", "dest": "/frontend/favicon.ico" },
    { "src": "/", "dest": "/frontend/index.html" },
    { "src": "/(.*)", "dest": "/frontend/index.html" }
  ]
}

